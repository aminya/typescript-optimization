{
  "name": "typescript-optimization",
  "version": "1.0.0",
  "scripts": {
    "typescript": "tsc || echo done",
    "babel": "babel src --out-dir src",
    "prettier": "prettier ./src/**/* --write",
    "build": "npm run typescript -s && npm run prettier -s"
  },
  "dependencies": {
    "benchmark": "^2.1.4",
    "chance": "^1.1.4"
  },
  "devDependencies": {
    "@types/chance": "^1.1.0",
    "typescript": "^3.8.3",
    "ts-node": "^8.7.0",
    "prettier": "^2.0.4",
    "babel-preset-minify": "^0.5.1",
    "@babel/plugin-transform-for-of": "^7.8.6",
    "@babel/core": "^7.8.7",
    "@babel/cli": "^7.8.4"
  },
  "prettier": {
    "semi": false,
    "printWidth": 120
  },
  "babel": {
    "presets": [
      [
        "minify",
        {
          "deadcode": false,
          "mangle": false
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-for-of",
        {
          "loose": true
        }
      ]
    ]
  }
}
